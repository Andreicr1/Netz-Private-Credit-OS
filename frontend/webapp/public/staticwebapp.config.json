{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated", "anonymous"]
    },
    {
      "route": "/.auth/*",
      "allowedRoles": ["authenticated", "anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.css", "/*.js", "/*.map", "/*.json", "/*.svg", "/*.png", "/*.ico", "/*.woff", "/*.woff2"]
  }
}
